   "source": [
    "def solution(numbers, target):\n",
    "    super=[0]\n",
    "    for i in numbers:\n",
    "        sub=[]\n",
    "        for j in super:\n",
    "            sub.append(j+i)\n",
    "            sub.append(j-i)\n",
    "        super=sub\n",
    "    return super.count(target)"
   ]
  }
